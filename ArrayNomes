package one.digitalinnovation.collections

fun main() {
    val nomes = Array(3) {""}
    nomes[0] = "Roni"
    nomes[1] = "Gabriel"
    nomes[2] = "Passos"

    for (nome in nomes) {
        println(nome)
    }

    println("----------------------")
    nomes.sort()
    nomes.forEach { println(it) }

    println("----------------------")
    val nomes2 = arrayOf("Roni", "Gabriel", "Passos")
    nomes2.sort()
    nomes2.forEach { println(it) }
}
